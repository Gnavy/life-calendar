{
  "search.exclude": {
    "**/node_modules": true,
    "dist/": true,
    "static/": true
  },
  "files.exclude": {
    "**/node_modules": true,
    "npm-debug.log*": true
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.associations": {
    "*.ttml": "xml",
    "*.ttss": "css"
  }
}